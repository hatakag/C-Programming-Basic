#include <stdio.h>
#include "StackArray.h"
#define Max 50
typedef int Eltype;
typedef Eltype StackType[Max];
//int top;

void initialize(StackType stack,int top) {
  top=0;
}

int empty(StackType stack,int top) {
  return top==0;
}

int full(StackType stack,int top) {
  return top==Max;
}

void push(Eltype el,StackType stack,int top) {
  if (full(stack,top))
    printf("stack overflow\n");
  else stack[top++]=el;
}

Eltype pop(StackType stack,int top) {
  if (empty(stack,top))
    printf("stack underflow\n");
  else return stack[--top];
}

Eltype top(StackType stack) {
  if (empty(stack)) {
    printf("stack overflow\n");
    return -999999;
  }
  else return stack[top-1];
}

